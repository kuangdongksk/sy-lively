    "rollup-plugin-livereload": "^2.0.5",
    "sass": "^1.63.3",
    "siyuan": "1.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "vite": "^5.0.0",
    "vite-plugin-static-copy": "^1.0.2",
    "vite-plugin-zip-pack": "^1.0.5"
  },
  "dependencies": {
    "@ant-design/colors": "^7.1.0",
    "@ant-design/icons": "^5.5.1",
    "@ant-design/pro-components": "^2.7.19",
    "@types/react": "^18.3.11",
    "@vitejs/plugin-react": "^4.3.2",
    "antd": "^5.21.2",
